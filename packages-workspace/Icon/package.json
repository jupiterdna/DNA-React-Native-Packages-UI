{
  "name": "@rndna/icon",
  "version": "0.0.0",
  "author": "Juphet Vitualla",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "package.json",
    "index.esm.js",
    "index.js"
  ],
  "typesVersions": {
    "*": {
      "outline": [
        "./dist/outline/index.d.ts"
      ],
      "solid": [
        "./dist/solid/index.d.ts"
      ],
      "outline/*": [
        "./dist/outline/*.d.ts"
      ],
      "solid/*": [
        "./dist/solid/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn gen && tsup",
    "gen": "ts-node --project tsconfig.json --files template/index.ts",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@rndna/base_style": "*",
    "@dnamobile/typescript-config": "*",
    "@types/react": "^18.2.38",
    "@types/react-native": "^0.72.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "junk": "^3.1.0",
    "ts-node": "^10.7.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0"
  },
  "dependencies": {
    "react-native-svg": "13.14.0",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*",
    "react-native-svg": "14.1.0"
  }
